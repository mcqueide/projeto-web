<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib
    PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"
    "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">

<taglib>
	<tlibversion>1.0</tlibversion>
	<jspversion>2.0</jspversion>
	<shortname>treinamento</shortname>
	<uri>http://www.foton.la/treinamento.tld</uri>
	<info>Tags de treinamento</info>

	<tag>
		<name>titulo</name>
		<tagclass>la.foton.treinamento.taglig.BarraTituloTagLib</tagclass>
		<info>Renderiza barra de títulos</info>
		<attribute>
			<name>valor</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

</taglib>